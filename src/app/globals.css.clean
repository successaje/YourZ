/* Base styles */
* {
  border-color: hsl(var(--color-border));
  transition-property: color, background-color, border-color, text-decoration-color, fill, stroke;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 200ms;
}

body {
  background-color: hsl(var(--color-background));
  color: hsl(var(--color-foreground));
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/* Typography */
h1 {
  scroll-margin-top: 5rem;
  font-size: 2.25rem;
  line-height: 2.5rem;
  font-weight: 800;
  letter-spacing: -0.025em;
}

h2 {
  scroll-margin-top: 5rem;
  border-bottom-width: 1px;
  padding-bottom: 0.5rem;
  font-size: 1.875rem;
  line-height: 2.25rem;
  font-weight: 600;
  letter-spacing: -0.025em;
}

h3 {
  scroll-margin-top: 5rem;
  font-size: 1.5rem;
  line-height: 2rem;
  font-weight: 600;
  letter-spacing: -0.025em;
}

h4 {
  scroll-margin-top: 5rem;
  font-size: 1.25rem;
  line-height: 1.75rem;
  font-weight: 600;
  letter-spacing: -0.025em;
}

p {
  line-height: 1.75rem;
}

p:not(:first-child) {
  margin-top: 1.5rem;
}

/* Components */
.btn-primary {
  padding: 0.5rem 1rem;
  background-color: rgb(37 99 235);
  color: white;
  border-radius: 0.5rem;
  transition-property: color, background-color, border-color, text-decoration-color, fill, stroke;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 150ms;
}

.btn-primary:hover {
  background-color: rgb(29 78 216);
}

.dark .btn-primary {
  background-color: rgb(59 130 246);
}

.dark .btn-primary:hover {
  background-color: rgb(37 99 235);
}

/* Card */
.card {
  background-color: white;
  border-radius: 0.75rem;
  box-shadow: 0 1px 2px 0 rgb(0 0 0 / 0.05);
  border: 1px solid hsl(var(--color-border));
  padding: 1.5rem;
}

.dark .card {
  background-color: hsl(240 3.7% 15.9%);
  border-color: hsl(240 3.7% 15.9%);
}

/* Input */
.input {
  width: 100%;
  padding: 0.5rem 1rem;
  border: 1px solid hsl(var(--color-border));
  border-radius: 0.5rem;
  background-color: hsl(var(--color-background));
  color: hsl(var(--color-foreground));
}

.input:focus {
  outline: 2px solid transparent;
  outline-offset: 2px;
  box-shadow: 0 0 0 2px hsl(var(--color-ring));
  border-color: hsl(var(--color-ring));
}

/* Label */
.label {
  display: block;
  font-size: 0.875rem;
  line-height: 1.25rem;
  font-weight: 500;
  color: hsl(var(--color-foreground));
  margin-bottom: 0.25rem;
}

/* Navigation */
.nav-link {
  color: hsl(var(--color-muted-foreground));
  transition-property: color, background-color, border-color, text-decoration-color, fill, stroke;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 150ms;
}

.nav-link:hover {
  color: hsl(var(--color-foreground));
}

/* Content Wrapper */
.content-wrapper {
  background-color: white;
  border-radius: 0.75rem;
  box-shadow: 0 1px 2px 0 rgb(0 0 0 / 0.05);
  border: 1px solid hsl(var(--color-border));
  padding: 1.5rem;
}

.dark .content-wrapper {
  background-color: hsl(240 3.7% 15.9%);
  border-color: hsl(240 3.7% 15.9%);
}

/* Scrollbar */
::-webkit-scrollbar {
  width: 8px;
  height: 8px;
}

::-webkit-scrollbar-track {
  background-color: hsl(240 4.9% 83.9% / 0.5);
}

.dark ::-webkit-scrollbar-track {
  background-color: hsl(240 3.7% 15.9%);
}

::-webkit-scrollbar-thumb {
  background-color: hsl(240 5.9% 90%);
  border-radius: 9999px;
}

::-webkit-scrollbar-thumb:hover {
  background-color: hsl(240 5.2% 81.8%);
}

.dark ::-webkit-scrollbar-thumb {
  background-color: hsl(240 3.7% 25.9%);
}

.dark ::-webkit-scrollbar-thumb:hover {
  background-color: hsl(240 3.7% 30.9%);
}

/* Line clamp utilities */
.line-clamp-2,
.line-clamp-3 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  line-clamp: 2;
  -webkit-line-clamp: 2;
  max-height: 3em;
}

.line-clamp-3 {
  line-clamp: 3;
  -webkit-line-clamp: 3;
  max-height: 4.5em;
}

/* Animations */
@keyframes gradient {
  0% {
    background-position: 0% 50%;
  }
  50% {
    background-position: 100% 50%;
  }
  100% {
    background-position: 0% 50%;
  }
}

@keyframes float {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}

@keyframes float-delayed {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}

@keyframes keys {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-2px);
  }
}

.animate-gradient {
  background-size: 200% 200%;
  animation: gradient 8s ease infinite;
}

.animate-float {
  animation: float 3s ease-in-out infinite;
}

.animate-float-delayed {
  animation: float-delayed 3s ease-in-out 0.5s infinite;
}

.animate-keys {
  animation: keys 1s ease-in-out infinite;
}

.animate-draw {
  stroke-dasharray: 1000;
  stroke-dashoffset: 1000;
  animation: draw 3s ease-in-out forwards;
}

@keyframes draw {
  to {
    stroke-dashoffset: 0;
  }
}

