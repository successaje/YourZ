/* RainbowKit Modal Styles */
:root {
  --rk-colors-connectButtonBackground: hsl(var(--background));
  --rk-colors-connectButtonBackgroundHover: hsl(var(--muted));
  --rk-colors-connectButtonText: hsl(var(--foreground));
  --rk-colors-connectButtonTextHover: hsl(var(--foreground));
  --rk-colors-connectButtonBorder: hsl(var(--border));
  
  --rk-colors-modalBackground: hsl(var(--popover));
  --rk-colors-modalText: hsl(var(--popover-foreground));
  --rk-colors-modalTextSecondary: hsl(var(--muted-foreground));
  --rk-colors-modalBackdrop: rgba(0, 0, 0, 0.5);
  
  --rk-colors-dropdownBackground: hsl(var(--popover));
  --rk-colors-dropdownText: hsl(var(--popover-foreground));
  --rk-colors-dropdownTextHover: hsl(var(--primary));
  --rk-colors-dropdownBackgroundHover: hsl(var(--muted));
  
  --rk-colors-connectionIndicator: hsl(var(--primary));
  --rk-colors-closeButton: hsl(var(--muted-foreground));
  --rk-colors-closeButtonHover: hsl(var(--foreground));
  
  --rk-shadows-connectButton: none;
  --rk-shadows-dialog: 0 4px 12px rgba(0, 0, 0, 0.1);
  
  --rk-radii-connectButton: 0.5rem;
  --rk-radii-modal: 0.75rem;
  --rk-radii-dropdownMenu: 0.5rem;
  
  --rk-fontSizes-modalTitle: 1.25rem;
  --rk-fontSizes-modalText: 1rem;
  --rk-fontSizes-modalTextSmall: 0.875rem;
}

/* Override RainbowKit modal styles */
.rk-connect-button {
  border-radius: var(--rk-radii-connectButton) !important;
  background-color: var(--rk-colors-connectButtonBackground) !important;
  color: var(--rk-colors-connectButtonText) !important;
  border: 1px solid var(--rk-colors-connectButtonBorder) !important;
  transition: all 0.2s ease !important;
}

.rk-connect-button:hover {
  background-color: var(--rk-colors-connectButtonBackgroundHover) !important;
  color: var(--rk-colors-connectButtonTextHover) !important;
}

/* Network Switcher Styles */
.network-switcher {
  background-color: hsl(var(--background));
  color: hsl(var(--foreground));
  border-color: hsl(var(--border));
  transition-property: background-color, border-color, color, fill, stroke, opacity, box-shadow, transform;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 150ms;
}

.network-switcher:hover {
  background-color: hsl(var(--muted));
}

.network-dropdown {
  background-color: hsl(var(--popover));
  color: hsl(var(--popover-foreground));
  border: 1px solid hsl(var(--border));
  box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05);
  border-radius: 0.5rem;
  overflow: hidden;
}

.network-item {
  padding-left: 1rem;
  padding-right: 1rem;
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
  font-size: 0.875rem;
  line-height: 1.25rem;
  transition-property: background-color, border-color, color, fill, stroke, opacity, box-shadow, transform;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 150ms;
  cursor: pointer;
  display: flex;
  align-items: center;
  gap: 0.5rem;
}

.network-item:hover {
  background-color: hsl(var(--muted));
}

.network-item:not(:last-child) {
  border-bottom: 1px solid hsl(var(--border));
}

.network-item[data-active="true"] {
  background-color: hsl(var(--muted));
  font-weight: 500;
}

.network-item[data-disabled="true"] {
  opacity: 0.5;
  cursor: not-allowed;
}

/* Line clamp utilities */
.line-clamp-1 {
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  line-clamp: 1;
  max-height: 1.5em;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.line-clamp-2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  line-clamp: 2;
  max-height: 3em;
  text-overflow: ellipsis;
}

@layer utilities {
  .animate-gradient {
    background-size: 200% 200%;
    animation: gradient 8s ease infinite;
  }
}

@keyframes gradient {
  0% {
    background-position: 0% 50%;
  }
  50% {
    background-position: 100% 50%;
  }
  100% {
    background-position: 0% 50%;
  }
}

@keyframes float {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}

@keyframes float-delayed {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}

@keyframes keys {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-2px);
  }
}

@keyframes draw {
  0% {
    stroke-dasharray: 1000;
    stroke-dashoffset: 1000;
  }
  100% {
    stroke-dasharray: 1000;
    stroke-dashoffset: 0;
  }
}

@keyframes pulse {
  0%, 100% {
    opacity: 1;
    transform: scale(1);
  }
  50% {
    opacity: 0.5;
    transform: scale(1.2);
  }
}

.animate-float {
  animation: float 3s ease-in-out infinite;
}

.animate-float-delayed {
  animation: float-delayed 3s ease-in-out infinite;
  animation-delay: 0.5s;
}

.animate-keys {
  animation: keys 1s ease-in-out infinite;
}

.animate-draw {
  animation: draw 2s ease-in-out forwards;
  animation-delay: 1s;
}

.animate-pulse {
  animation: pulse 2s ease-in-out infinite;
} 